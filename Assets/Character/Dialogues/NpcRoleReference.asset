  - m_object:
      m_preconditionName: 
      m_userdata: 0
      m_lines:
      - m_text: Isn't all this fascinating?
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      m_loop: 0
      m_singleUse: 0
      m_appendToAll: 0
