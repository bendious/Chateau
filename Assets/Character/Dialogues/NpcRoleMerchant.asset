      m_preconditionName: 
      m_userdata: 0
      m_relationshipMin: 0
      m_relationshipMax: 1
      m_relationshipIncrement: 0
      m_lines:
      - m_text: What, you want something?
        m_source: {fileID: 0}
        m_sourceDistMax: 3.402823e+38
        m_replies: []
      - m_text: Well I need materials to work with first.
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      - m_text: You bring me enough junk you find lying around, and I'll rig up whatever
          you want.
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      - m_text: I can't salvage everything, so don't expect to swap things around
          without loss -
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      - m_text: But if you want something specific that's not on hand, remember me
          first!
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      m_target: {fileID: 0}
      m_loop: 0
      m_singleUse: 1
      m_appendToAll: 0
    m_weight: 0
  - m_object:
