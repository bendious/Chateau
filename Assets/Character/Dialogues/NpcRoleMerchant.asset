      m_preconditionName: 
      m_userdata: 0
      m_relationshipMin: 0
      m_relationshipMax: 1
      m_relationshipIncrement: 0
      m_lines:
      - m_text: Oh, and another thing.
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      - m_text: My workshop's down in the old laundry room.
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      - m_text: So remember that you don't have to make the trek all the way back
          here just to give me stuff.
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      - m_text: Just find a laundry chute and toss whatever you want down; I'll find
          it.
        m_source: {fileID: 0}
        m_sourceDistMax: 3.4028235e+38
        m_replies: []
      m_target: {fileID: 0}
      m_loop: 0
      m_singleUse: 1
      m_appendToAll: 0
    m_weight: 0
